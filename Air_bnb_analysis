#Pie Chart
In [96]:
plt.figure(figsize=(10,6))
Total_rooms=Airbnb_df['room_type'].value_counts()
labels=Total_rooms.index
sizes=Total_rooms.values
plt.pie(sizes,labels=labels,autopct='%1.1f%%')
plt.legend(title='Room_type', bbox_to_anchor=(0.8,0,0.5,1), fontsize=12)
plt.show()
 




